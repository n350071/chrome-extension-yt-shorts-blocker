{
  "manifest_version": 3,
  "name": "YouTube Shorts Blocker",
  "version": "1.0",
  "description": "Redirect YouTube Shorts to blank page.",
  "permissions": ["declarativeNetRequest"],
  "host_permissions": ["*://www.youtube.com/*"],
  "action": {
    "default_title": "Shorts Blocker"
  },
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  }
}
